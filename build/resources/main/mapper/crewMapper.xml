<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ssd.springcooler.gachiwatch.dao.mybatis.mapper.CrewMapper">
    <cache/>
    <select id="getCrewByCrewId" resultType="ssd.springcooler.gachiwatch.domain.Crew">
        SELECT *
        FROM Crew
        WHERE crew_id = #{crewId}
    </select>
</mapper>